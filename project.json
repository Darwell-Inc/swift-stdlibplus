{
  "name": "CoreUtils",
  "options": {
    "groupOrdering": [
      {
        "order": [
          "Sources",
          "Tests"
        ]
      }
    ],
    "deploymentTarget": {
      "iOS": "14.5"
    },
    "groupSortPosition": "top",
    "tabWidth": 3,
    "indentWidth": 3,
    "bundleIdPrefix": "com.galenit",
    "defaultConfig": "Release"
  },
  "configs": {
    "Debug": "debug",
    "Release": "release"
  },
  "settings": {
    "base": {
      "GENERATE_INFOPLIST_FILE": "YES",
      "ARCHS": "arm64",
      "OWNER_NAME": "GalenIT",
      "CODE_SIGN_STYLE": "Automatic",
      "SUPPORTS_MACCATALYST": "NO",
      "SWIFT_VERSION": "5.4",
      "TARGETED_DEVICE_FAMILY": "1",
      "DEVELOPMENT_TEAM": "SL34K73738"
    }
  },
  "settingGroups": {
    "versioningSettings": {
      "MARKETING_VERSION": "1.0",
      "CURRENT_PROJECT_VERSION": "10"
    },
    "baseSettings": {
    }
  },
  "configFiles": {
  },
  "targets": {
    "GalenIT_CoreSDK": {
      "buildVersion": 1.2,
      "deploymentTarget": "14.5",
      "info": {
        "path": "Manifests/Info.plist",
        "properties": {
          "CFBundlePackageType": "$(PRODUCT_BUNDLE_PACKAGE_TYPE)",
          "CFBundleExecutable": "$(EXECUTABLE_NAME)",
          "NSHumanReadableCopyright": "$(OWNER_NAME)",
          "CFBundleDevelopmentRegion": "$(DEVELOPMENT_LANGUAGE)",
          "CFBundleInfoDictionaryVersion": "6.0",
          "CFBundleName": "$(PRODUCT_NAME)",
          "CFBundleIdentifier": "$(PRODUCT_BUNDLE_IDENTIFIER)",
          "CFBundleVersion": "$(CURRENT_PROJECT_VERSION)",
          "CFBundleShortVersionString": "$(MARKETING_VERSION)"
        }
      },
      "platform": "iOS",
      "settings": {
        "groups": [
          "versioningSettings",
          "baseSettings"
        ],
        "base": {
          "SWIFT_OBJC_INTERFACE_HEADER_NAME": "$(SWIFT_MODULE_NAME)-Swift.h",
          "CODE_SIGN_IDENTITY": "Apple Development",
            "SKIP_INSTALL": "NO",
          "TARGETED_DEVICE_FAMILY": "1",
          "IBSC_MODULE": "GalenIT_CoreSDK",
          "MACH_O_TYPE": "mh_dylib",
          "ENABLE_BITCODE": "YES",
          "DYLIB_CURRENT_VERSION": "$(CURRENT_PROJECT_VERSION)",
          "DEFINES_MODULE": "YES",
          "BUILD_LIBRARY_FOR_DISTRIBUTION": "YES"
        },
        "configs": {
          "Release": {
            "SWIFT_TREAT_WARNINGS_AS_ERRORS": "NO",
            "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited)"
          },
          "Debug": {
            "SWIFT_TREAT_WARNINGS_AS_ERRORS": "NO",
            "SWIFT_ACTIVE_COMPILATION_CONDITIONS": "$(inherited) DEBUG"
          }
        }
      },
      "sources": [
        "Sources"
      ],
      "type": "framework"
    },
    "Tests": {
      "type": "bundle.unit-test",
      "platform": "iOS",
      "deploymentTarget": "14.5",
      "sources": [
        "Tests"
      ],
      "settings": {
        "base": {
          "HEADER_SEARCH_PATHS": "\"$(TARGET_TEMP_DIR)/../$(PROJECT_NAME).build/DerivedSources\"",
          "SKIP_INSTALL": "NO",
          "CODE_SIGN_IDENTITY": "Apple Development"
        },
        "configs": {
          "Release": {
          },
          "Debug": {
          }
        },
        "groups": [
          "baseSettings"
        ]
      }
    }
  },
  "schemes": {
    "CoreUtils": {
      "build": {
        "targets": {
          "GalenIT_CoreSDK": "all"
        }
      },
      "run": {
        "config": "Debug",
        "commandLineArguments": {
          "-FIRAnalyticsDebugEnabled": "YES",
          "-FIRDebugDisabled": "YES"
        }
      },
      "test": {
        "config": "Debug",
        "commandLineArguments": {
          "-FIRAnalyticsDebugEnabled": "YES",
          "-FIRDebugDisabled": "YES"
        },
        "gatherCoverageData": true,
        "targets": [
          {
            "name": "Tests",
            "parallelizable": true,
            "randomExecutionOrder": true
          }
        ]
      },
      "profile": {
        "config": "Release"
      },
      "analyze": {
        "config": "Debug"
      },
      "archive": {
        "config": "Release",
        "revealArchiveInOrganizer": true
      }
    }
  }}